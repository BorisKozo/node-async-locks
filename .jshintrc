{
    "maxerr": 200,
    "bitwise": true,
    "camelcase": false,
    "curly": true,
    "eqeqeq": true,
    "forin": false,
    "immed": false,
    "indent": 4,
    "latedef": true,
    "newcap": false,
    "noarg": true,
    "noempty": false,
    "nonew": true,
    "plusplus": false,
    "quotmark": "single",
    "undef": true,
    "unused": true,
    "strict": true,
    "trailing": true,
    "maxparams": false,
    "maxdepth": false,
    "maxstatements": false,
    "maxcomplexity": 27,
    "maxlen": false,

    "evil": false,
    "expr": true,
    "funcscope": false,
    "globalstrict": true,
    "iterator": false,
    "lastsemic": false,
    "laxbreak": false,
